{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/leaderboard",
      "destination": "/leaderboard.html"
    },
    {
      "source": "/competition",
      "destination": "/competition.html"
    },
    {
      "source": "/resurrector",
      "destination": "/resurrector.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}